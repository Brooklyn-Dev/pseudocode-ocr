{
	"name": "pseudocode-ocr",
	"displayName": "pseudocode-ocr",
	"description": "A syntax highlighter and opinionated superset of the OCR A-level Computer Science Specification",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.101.0"
	},
	"categories": [
		"Programming Languages"
	],
	"main": "./src/extension.js",
	"type": "module",
	"contributes": {
		"languages": [
			{
				"id": "ocr-pseudocode",
				"aliases": [
					"OCR Pseudocode",
					"pseudocode"
				],
				"extensions": [
					".pseudo",
					".pseudocode",
					".ocr"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "ocr-pseudocode-strict",
				"aliases": [
					"OCR Pseudocode (Strict)",
					"pseudocode-strict"
				],
				"extensions": [
					".pseudo",
					".pseudocode",
					".ocr"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ocr-pseudocode",
				"scopeName": "source.ocr-pseudocode",
				"path": "./syntaxes/ocr-pseudocode.tmLanguage.json"
			},
			{
				"language": "ocr-pseudocode-strict",
				"scopeName": "source.ocr-pseudocode-strict",
				"path": "./syntaxes/ocr-pseudocode-strict.tmLanguage.json"
			}
		],
		"snippets": [
			{
				"language": "ocr-pseudocode",
				"path": "./snippets/ocr-pseudocode.json"
			},
			{
				"language": "ocr-pseudocode-strict",
				"path": "./snippets/ocr-pseudocode.json"
			}
		]
	}
}
